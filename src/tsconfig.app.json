{
	"compilerOptions": {
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": false,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"strictNullChecks": false,
		"lib": [
			"es2017",
			"dom"
		],
		"outDir": "../out-tsc/app",
		"target": "es5",
		"module": "es2015",
		"baseUrl": "",
		"types": [
			"hammerjs"
		]
	},
	"exclude": [
		"test.ts",
		"**/*.spec.ts"
	]
}
